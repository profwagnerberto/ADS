 using System;
 using Fintech;

 class MainClass {

   public static void Main (string[] args) {
     Console.Clear();
     ContaCorrente conta0 = new ContaCorrente();
     conta0.numero = 11;
     conta0.titular = "Bruno";
     conta0.saldo = 0;
     Console.WriteLine("Conta{0} | Saldo: {1,6:F2}",
      0, conta0.PegarSaldo() );

   }
 }
