using System;

class MainClass {
  public static void Main (string[] args) {
    char um = 'M';
    string Kg = "Quilograma";
    int dz = 10;
    double e = 2.71;
    bool tt = true;
    Console.WriteLine("Metro: " + um);
    Console.WriteLine("Peso: " + Kg);
    Console.WriteLine("Dezena: " + dz);
    Console.WriteLine("Napier: " + e);
    Console.WriteLine("Tautologia: " + tt);
  }
}