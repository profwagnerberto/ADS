using System;

class MainClass {
  public static void Main (string[] args) {
    var c1 = 1 - 1.0 / 2.0;
    var c2 = 9 != 9;
    var c3 = true || false;
    Console.WriteLine( $"Cálculo 1: { c1 }" );
    Console.WriteLine( $"Cálculo 2: { c2 }" );
    Console.WriteLine( $"Cálculo 3: { c3 }" );
  }
}